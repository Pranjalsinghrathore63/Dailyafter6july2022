package tutorials;
import java.util.*;


public class leapyear {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		int year = 2100;
	//check whether this is leap year or not 
		if(year % 400 == 0)
		{
			System.out.println("  yes this is leap year");
		}
		else if(year % 100== 0) {
			System.out.println("this is not leap year");
		}
		else if(year % 4 == 0) {
			System.out.println(" this leap year");
		}
		
		else {
			System.out.println("wrong input year");
		}

	}

}

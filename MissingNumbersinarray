package Arrays2022;
import java.util.*;
import java.util.Arrays;



public class MissingNumbers {
	
	
	
	public static int find(int[] nums) {
		
		int n = nums.length;
		
	int sum = ((n+1 )*(n+2))/2;
	for(int i=0 ; i<n ; i++)
	
		sum -= nums[i];// sum sum - nums[i]
	
		return sum;
	
				
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		
		int[] a = { 1, 2, 4,5,6};
		System.out.println(find(a));
		
		

	}

}
